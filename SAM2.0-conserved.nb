Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "kappace", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Options", "[", "kappace", "]"}], "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"PrintMode", "\[Rule]", "False"}], ",", 
     RowBox[{"PrecomputedRules", "\[Rule]", "0"}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"kappace", "[", 
    RowBox[{
     RowBox[{"n_", "?", "IntegerQ"}], ",", 
     RowBox[{"opts", ":", 
      RowBox[{"OptionsPattern", "[", "]"}]}]}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"lockappacerules", "=", 
        RowBox[{"OptionValue", "[", "PrecomputedRules", "]"}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"expr", "=", "\"\<\>\""}]}], "\[IndentingNewLine]", "}"}], ",",
      "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"lockappacerules", "\[Equal]", "0"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"lockappacerules", "=", 
         RowBox[{"allkappasce", "[", "n", "]"}]}]}], "\[IndentingNewLine]", 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"OptionValue", "[", "PrintMode", "]"}], ",", 
