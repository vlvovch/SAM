Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"allkappasce", "[", 
    RowBox[{"nmax_", ":", "6"}], "]"}], ":=", "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"dfs", ",", "res", ",", "tmp", ",", "rfin", ",", "rfout"}], 
      "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"dfs", "=", 
       RowBox[{"dfstokappas", "[", "nmax", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"res", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"kappanotation", "[", 
          RowBox[{"1", ",", "in", ",", "ce"}], "]"}], "\[Rule]", 
         RowBox[{"kappanotation", "[", 
          RowBox[{"1", ",", "in", ",", "gce"}], "]"}]}], "}"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"rfin", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"fnotation", "[", 
           RowBox[{"i", ",", "in"}], "]"}], "\[Rule]", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"fnotation", "[", "i", "]"}], "/.", "dfs"}], ")"}], "/.", 
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{
               SubscriptBox["\[Kappa]", "j"], "\[Rule]", 
               RowBox[{"kappanotation", "[", 
                RowBox[{"j", ",", "in", ",", "gce"}], "]"}]}], ",", 
              RowBox[{"{", 
               RowBox[{"j", ",", "1", ",", "nmax"}], "}"}]}], "]"}]}], 
           ")"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", "nmax"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"rfout", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"fnotation", "[", 
           RowBox[{"i", ",", "out"}], "]"}], "\[Rule]", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"fnotation", "[", "i", "]"}], "/.", "dfs"}], ")"}], "/.", 
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{
               SubscriptBox["\[Kappa]", "j"], "->", 
               RowBox[{"kappanotation", "[", 
                RowBox[{"j", ",", "out", ",", "gce"}], "]"}]}], ",", 
              RowBox[{"{", 
               RowBox[{"j", ",", "1", ",", "nmax"}], "}"}]}], "]"}]}], 
           ")"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", "nmax"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"tmp", "=", 
          RowBox[{
           FractionBox["1", 
            RowBox[{
             RowBox[{"fnotation", "[", 
              RowBox[{"2", ",", "in"}], "]"}], "+", 
             RowBox[{"fnotation", "[", 
              RowBox[{"2", ",", "out"}], "]"}]}]], 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"KroneckerDelta", "[", 
              RowBox[{"n", ",", "1"}], "]"}], "-", 
             RowBox[{"Sum", "[", 
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{"fnotation", "[", 
                  RowBox[{
                   RowBox[{"k", "+", "1"}], ",", "in"}], "]"}], "*", 
                 RowBox[{"BellY", "[", 
                  RowBox[{"n", ",", "k", ",", 
                   RowBox[{"Table", "[", 
                    RowBox[{
                    RowBox[{"kappanotation", "[", 
                    RowBox[{"l", ",", "in", ",", "ce"}], "]"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"l", ",", "2", ",", 
                    RowBox[{"n", "-", "k", "+", "2"}]}], "}"}]}], "]"}]}], 
                  "]"}]}], "-", 
                RowBox[{
                 RowBox[{"fnotation", "[", 
                  RowBox[{
                   RowBox[{"k", "+", "1"}], ",", "out"}], "]"}], "*", 
                 RowBox[{"BellY", "[", 
                  RowBox[{"n", ",", "k", ",", 
                   RowBox[{"Table", "[", 
                    RowBox[{
                    RowBox[{"-", 
                    RowBox[{"kappanotation", "[", 
                    RowBox[{"l", ",", "in", ",", "ce"}], "]"}]}], ",", 
                    RowBox[{"{", 
                    RowBox[{"l", ",", "2", ",", 
                    RowBox[{"n", "-", "k", "+", "2"}]}], "}"}]}], "]"}]}], 
                  "]"}]}]}], ",", 
               RowBox[{"{", 
                RowBox[{"k", ",", "2", ",", "n"}], "}"}]}], "]"}]}], 
            ")"}]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"tmp", "=", 
          RowBox[{
           RowBox[{
            RowBox[{"tmp", "/.", "res"}], "/.", "rfin"}], "/.", "rfout"}]}], 
         ";", "\[IndentingNewLine]", 
         RowBox[{"tmp", "=", 
          RowBox[{"Simplify", "[", "tmp", "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"res", ",", 
           RowBox[{
            RowBox[{"kappanotation", "[", 
             RowBox[{
              RowBox[{"n", "+", "1"}], ",", "in", ",", "ce"}], "]"}], 
            "\[Rule]", "tmp"}]}], "]"}], ";"}], "\[IndentingNewLine]", ",", 
        "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"n", ",", "1", ",", 
          RowBox[{"nmax", "-", "1"}]}], "}"}]}], "]"}], ";", 
      "\[IndentingNewLine]", "res"}]}], "\[IndentingNewLine]", "]"}]}], 
  ";"}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.80308810100745*^9, 3.8030881652307014`*^9}, {
