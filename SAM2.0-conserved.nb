Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"dfstokappas", "[", 
    RowBox[{"nmax_", ":", "6"}], "]"}], ":=", "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"res", ",", "tmp"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"res", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"fnotation", "[", "1", "]"}], "\[Rule]", "0"}], "}"}]}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"tmp", "=", 
          FractionBox[
           RowBox[{
            RowBox[{"KroneckerDelta", "[", 
             RowBox[{"n", ",", "1"}], "]"}], "-", 
            RowBox[{"Sum", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"fnotation", "[", 
                RowBox[{"k", "+", "1"}], "]"}], "*", 
               RowBox[{"BellY", "[", 
                RowBox[{"n", ",", "k", ",", 
                 RowBox[{"Table", "[", 
                  RowBox[{
                   SubscriptBox["\[Kappa]", "l"], ",", 
                   RowBox[{"{", 
                    RowBox[{"l", ",", "2", ",", 
                    RowBox[{"n", "-", "k", "+", "2"}]}], "}"}]}], "]"}]}], 
                "]"}]}], ",", 
              RowBox[{"{", 
               RowBox[{"k", ",", "1", ",", 
                RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}]}], 
           SuperscriptBox[
            RowBox[{"(", 
             SubscriptBox["\[Kappa]", "2"], ")"}], "n"]]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"tmp", "=", 
          RowBox[{"tmp", "/.", "res"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"tmp", "=", 
          RowBox[{"Simplify", "[", "tmp", "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"res", ",", 
           RowBox[{
            RowBox[{"fnotation", "[", 
             RowBox[{"n", "+", "1"}], "]"}], "\[Rule]", "tmp"}]}], "]"}], 
         ";"}], "\[IndentingNewLine]", ",", "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"n", ",", "1", ",", 
          RowBox[{"nmax", "-", "1"}]}], "}"}]}], "]"}], ";", 
      "\[IndentingNewLine]", "res"}]}], "\[IndentingNewLine]", "]"}]}], 
  ";"}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.803087169381686*^9, 3.8030873800639706`*^9}, {
